msgid "Copy this code to clipboard"
msgstr "העתק את הקוד"

#: ../includes/configs/strings.php:7
msgid "Open this code in a new window"
msgstr "פתח את הקוד בחלון חיצוני"

#: ../includes/configs/strings.php:8
msgid "Wrap lines"
msgstr "קבץ שורות"

#: ../includes/configs/strings.php:9
msgid "Show code"
msgstr "הצג קוד"
